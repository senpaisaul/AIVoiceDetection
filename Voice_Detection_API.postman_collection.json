{
  "info": {
    "name": "Voice Detection API",
    "description": "External testing collection for Voice Detection API",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "base_url",
      "value": "https://YOUR_DEPLOYED_URL.onrender.com",
      "type": "string"
    },
    {
      "key": "api_key",
      "value": "hackathon-voice-api-2024-secure-key",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "1. Health Check",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/health",
          "host": ["{{base_url}}"],
          "path": ["health"]
        }
      },
      "response": []
    },
    {
      "name": "2. Valid Voice Detection (English)",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "x-api-key",
            "value": "{{api_key}}",
            "type": "text"
          },
          {
            "key": "Content-Type",
            "value": "application/json",
            "type": "text"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"language\": \"English\",\n  \"audioFormat\": \"mp3\",\n  \"audioBase64\": \"\"\n}"
        },
        "url": {
          "raw": "{{base_url}}/api/voice-detection",
          "host": ["{{base_url}}"],
          "path": ["api", "voice-detection"]
        }
      },
      "response": []
    },
